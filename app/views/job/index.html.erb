<div class="row">
  <h1>Jobs List</h1>
</div>

<div class="row">
  <table class="table table-striped table-bordered">
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Required Skills</th>
    </tr>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job[:id] %></td>
        <td><%= job[:title] %></td>
        <td><%= job[:required_skills] %></td>
      </tr>
    <% end %>
  </table>
</div>